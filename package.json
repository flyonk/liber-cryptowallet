{
  "name": "cw",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "format": "yarn prettier && yarn lint:all:fix",
    "ios:open": "npx cap open ios",
    "ios:run": "npx cap run ios",
    "lint-fix": "yarn lint:code:fix && yarn lint:style:fix",
    "lint:all:fix": "yarn lint:style:fix && yarn lint:code:fix",
    "lint:all": "yarn lint:style && yarn lint:code",
    "lint:code:fix": "yarn lint:code --fix",
    "lint:code": "vue-cli-service lint src/**/*.{ts,vue} tests/**/*.ts",
    "lint:style:fix": "yarn lint:style --fix",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\"",
    "lint": "yarn lint:code && yarn lint:style",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,vue,htm,html,json,xml,css,scss,sass}' '*.{js,jsx,ts,tsx,vue,htm,html,json,xml,css,scss,sass}'",
    "serve": "vue-cli-service serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@capacitor/core": "^3.3.4",
    "@capacitor/ios": "^3.3.4",
    "@capacitor/push-notifications": "^1.0.9",
    "@types/axios": "^0.14.0",
    "axios": "^0.24.0",
    "core-js": "^3.6.5",
    "pinia": "^2.0.9",
    "primeflex": "^3.1.2",
    "primevue": "^3.10.0",
    "qrcode-with-logos": "^1.0.3",
    "register-service-worker": "^1.7.1",
    "vue": "^3.2.22",
    "vue-agile": "^2.0.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@capacitor/cli": "^3.3.4",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/cli-plugin-babel": "~5.0.0-rc.1",
    "@vue/cli-plugin-e2e-cypress": "~5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.1",
    "@vue/cli-plugin-pwa": "~5.0.0-rc.1",
    "@vue/cli-plugin-router": "~5.0.0-rc.1",
    "@vue/cli-plugin-typescript": "~5.0.0-rc.1",
    "@vue/cli-plugin-unit-jest": "~5.0.0-rc.1",
    "@vue/cli-service": "~5.0.0-rc.1",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^27.0.7",
    "typescript": "~4.4.4",
    "vue-jest": "^5.0.0-0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint",
      "git add"
    ],
    "*.{css,vue}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  }
}

{
  "name": "cw",
  "version": "1.2.0",
  "private": true,
  "brand": "liber",
  "scripts": {
    "serve": "cross-env process.env.VUE_APP_BRAND='liber' vue-cli-service serve",
    "build": "cross-env process.env.VUE_APP_BRAND='liber' vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --config=jest-unit.config.js",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "yarn test:unit",
    "lint": "cross-env process.env.VUE_APP_BRAND='liber' yarn lint:code && yarn lint:style",
    "devtools": "PORT=8020 vue-devtools",
    "format": "yarn prettier && yarn lint:all:fix",
    "ios:open": "npx cap open ios",
    "ios:run": "npx cap run ios",
    "ios:sync": "dotenv -e .env.dev npx cap sync",
    "lint-fix": "yarn lint:code:fix && yarn lint:style:fix",
    "lint:all": "yarn lint:style && yarn lint:code",
    "lint:all:fix": "yarn lint:style:fix && yarn lint:code:fix",
    "lint:code": "vue-cli-service lint src/**/*.{ts,vue} tests/**/*.ts",
    "lint:code:fix": "yarn lint:code --fix",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\"",
    "lint:style:fix": "yarn lint:style --fix",
    "postinstall": "husky install",
    "prepare": "husky install",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,vue,htm,html,json,xml,css,scss,sass}' '*.{js,jsx,ts,tsx,vue,htm,html,json,xml,css,scss,sass}'",
    "start:whitelabel": "./moduleinstall.sh",
    "env:from:json": "node libs/index.js",
    "peer": "add-no-save --peer-deps"
  },
  "dependencies": {
    "@capacitor-community/camera-preview": "3.0.0",
    "@capacitor-community/contacts": "^1.1.3",
    "@capacitor/app": "^1.1.1",
    "@capacitor/camera": "^1.3.0",
    "@capacitor/clipboard": "^1.0.8",
    "@capacitor/core": "^3.3.4",
    "@capacitor/device": "^1.1.2",
    "@capacitor/dialog": "^1.0.7",
    "@capacitor/ios": "^3.3.4",
    "@capacitor/project": "^2.0.10",
    "@capacitor/push-notifications": "^1.0.9",
    "@capacitor/share": "^1.1.2",
    "@capacitor/storage": "^1.2.4",
    "@sentry/browser": "^6.19.2",
    "@sentry/tracing": "^6.18.1",
    "@sentry/vue": "^6.18.1",
    "@sentry/webpack-plugin": "^1.18.8",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.179",
    "axios": "^0.25.0",
    "capacitor-native-biometric": "^3.0.3",
    "capacitor-native-settings": "^0.2.0",
    "capacitor-plugin-safe-area": "^0.0.3",
    "capacitor-secure-storage-plugin": "^0.6.2",
    "core-js": "^3.6.5",
    "crypto-browserify": "^3.12.0",
    "floating-vue": "^2.0.0-beta.6",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "path": "^0.12.7",
    "pinia": "^2.0.9",
    "primeflex": "^3.1.2",
    "primeicons": "^5.0.0",
    "primevue": "^3.10.0",
    "qrcode-vue3": "^1.4.17",
    "qrcode-with-logos": "^1.0.3",
    "register-service-worker": "^1.7.1",
    "stream-browserify": "^3.0.0",
    "swiper": "^8.1.0",
    "vue": "^3.2.22",
    "vue-agile": "^2.0.0",
    "vue-i18n": "9",
    "vue-router": "^4.0.0-0",
    "vue-translation-manager": "^1.2.0",
    "vue3-circle-progress": "^1.0.7",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-range-requests": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@capacitor/cli": "^3.3.4",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^27.4.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/luxon": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-e2e-cypress": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-pwa": "^5.0.4",
    "@vue/cli-plugin-router": "^5.0.4",
    "@vue/cli-plugin-typescript": "^5.0.4",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "yarn-add-no-save": "^1.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint"
    ],
    "*.{css,vue}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "commitlint": {
    "rules": {
      "references-empty": [
        2,
        "never"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "deploy",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "test"
        ]
      ],
      "type-empty": [
        2,
        "never"
      ]
    },
    "parserPreset": {
      "parserOpts": {
        "issuePrefixes": [
          "#"
        ]
      }
    }
  }
}
